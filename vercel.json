{
  "version": 2,
  "buildCommand": "cd our_solution/frontend && npm run build",
  "installCommand": "cd our_solution/frontend && npm install",
  "outputDirectory": "our_solution/frontend/dist",
  "rewrites": [
    {
      "source": "/api/tickets/:id/permanent",
      "destination": "/api/tickets"
    },
    {
      "source": "/api/tickets/:id/close",
      "destination": "/api/tickets"
    },
    {
      "source": "/api/tickets/:id",
      "destination": "/api/tickets"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
